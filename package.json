{
  "name": "koa-typescript-cli",
  "version": "0.0.24",
  "bin": {
    "koa-ts-cli": "./dist/bin/koa-ts-cli.js"
  },
  "scripts": {
    "build": "rm -rf dist && tsc && mkdir -p dist/src && cp -r src/template dist/src/template",
    "watch": "tsc -w",
    "dev": "npx tsx bin/koa-ts-cli.ts",
    "format": "prettier --write \"src/**/*.ts\" \"bin/**/*.ts\"",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SuPeiSen/koa-ts-cli"
  },
  "dependencies": {
    "@babel/generator": "^7.27.0",
    "@babel/parser": "^7.27.0",
    "@babel/template": "^7.27.0",
    "@babel/traverse": "^7.27.0",
    "@babel/types": "^7.27.0",
    "chalk": "^4.1.2",
    "chokidar": "^4.0.3",
    "commander": "^9.4.1",
    "cross-env": "^7.0.3",
    "dotenv": "^17.2.3",
    "glob": "^11.0.1",
    "inquirer": "^8.2.5",
    "jiti": "^2.6.1",
    "nodemon": "^3.1.9",
    "ora": "^6.1.2",
    "prettier": "^3.5.3",
    "reflect-metadata": "^0.2.2",
    "simple-git": "^3.27.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0"
  },
