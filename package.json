{
  "name": "koa-typescript-cli",
  "version": "0.0.25",
  "bin": {
    "koa-ts-cli": "./dist/bin/koa-ts-cli.js"
  },
  "scripts": {
    "build": "rm -rf dist && tsc && mkdir -p dist/src && cp -r src/template dist/src/template",
    "watch": "tsc -w",
    "dev": "npx tsx bin/koa-ts-cli.ts",
    "format": "prettier --write \"src/**/*.ts\" \"bin/**/*.ts\"",
    "prepublishOnly": "npm run build"
  },
  "description": "A powerful CLI tool for scaffolding and managing enterprise-grade Koa + TypeScript applications.",
  "keywords": [
    "koa",
    "koa3",
    "typescript",
    "cli",
    "scaffold",
    "framework",
    "generator",
    "swagger",
    "openapi"
  ],
  "homepage": "https://github.com/SuPeiSen/koa-ts-cli#readme",
  "bugs": {
    "url": "https://github.com/SuPeiSen/koa-ts-cli/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SuPeiSen/koa-ts-cli.git"
  },
  "license": "MIT",
  "author": "SuPeiSen",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@10.13.1+sha512.37ebf1a5c7a30d5fabe0c5df44ee8da4c965ca0c5af3dbab28c3a1681b70a256218d05c81c9c0dcf767ef6b8551eb5b960042b9ed4300c59242336377e01cfad"
}